<main id="main" class="main">

    <div class="pagetitle">
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/admin">Admin</a></li>
                <li class="breadcrumb-item">Price</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->

    <section class="section">
        <div class="row">
            <div class="col-lg-12">

                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Price Table</h5>
                        <!-- Primary Color Bordered Table -->
                        <table class="table table-bordered border-primary">
                            <thead>
                                <tr>
                                    <th scope="col" style="text-align:center;">Title</th>
                                    <th scope="col" style="text-align:center;">Details</th>
                                    <th scope="col" style="text-align:center;">Details2</th>
                                    <th scope="col" style="text-align:center;">Details3</th>
                                    <th scope="col" style="text-align:center;">Details4</th>
                                    <th scope="col" style="text-align:center;">Details5</th>
                                    <th scope="col" style="text-align:center;">Details6</th>
                                    <th scope="col" style="text-align:center;">Modify</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each price}}
                                <tr>
                                    <td>{{title}}</td>
                                    <td>{{details}}</td>
                                    <td>{{details2}}</td>
                                    <td>{{details3}}</td>
                                    <td>{{details4}}</td>
                                    <td>{{details5}}</td>
                                    <td>{{details6}}</td>
                                    <td style="text-align:center;">
                                        <a href="/admin/price/{{id}}/show" class="btn btn-outline-success btn-sm">
                                            <i class="bi bi-eye-fill"></i></a>
                                        <a href="/admin/price/{{id}}/edit" class="btn btn-outline-warning btn-sm"><i
                                                class="bi bi-pencil-square"></i></a>
                                        <a href="/admin/price/{{id}}/delete" class="btn btn-outline-danger btn-sm"><i
                                                class="bi bi-trash3-fill"></i></a>
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                        <!-- End Primary Color Bordered Table -->
                        <a href="/admin" class="btn btn-outline-success btn-sm">HOME</a>
                        <a href="/admin/price/create" class="btn btn-outline-success btn-sm">ADD PRICE</a>


                    </div>
                </div>
            </div>
    </section>

</main>
<!-- End #main -->